fn main(){
 
    let sum: fn(i32, i32) -> i32  = |a, b| a + b;
     
    do_operation(6, 4, sum);
    do_operation(6, 4, multiply);
}
fn do_operation(a: i32, b: i32, operation: fn(i32, i32) -> i32){
     
    let result = operation(a, b);
    println!("result: {}", result);
}
fn multiply(a: i32, b: i32) -> i32{
    a * b
}
